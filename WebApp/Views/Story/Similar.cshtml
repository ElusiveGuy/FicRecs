@model StorySimilarViewModel

@{
    ViewData["Title"] = "Index";
}

<h2>Selected Fic</h2>

@await Html.PartialAsync("_StoryListPartial", new StoryListViewModel { Fics = Model.SelectedFicAsEnumerable, ShowDetailed = false, ShowSimilarButton = false })

<h2>Similar Fics</h2>

@await Html.PartialAsync("_StoryListPartial", new StoryListViewModel { Fics = Model.SimilarFics, ShowDetailed = false, ShowSimilarButton = true })
